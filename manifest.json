{
    "manifest_version": 3,
    "name": "Tab Manager for Projects",
    "version": "1.0",
    "description": "Organize and manage tabs by projects.",
    "permissions": ["tabs", "storage"],
    "host_permissions": [
  "<all_urls>"
],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  